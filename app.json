{
  "expo": {
    "name": "user",
    "slug": "user",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/rickshaw.png",
    "scheme": "user",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBVuu_nVQ1WuEGf3rUur15LKBzz5o_OgP0"
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBVuu_nVQ1WuEGf3rUur15LKBzz5o_OgP0"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/carIcon.jpg",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/icons/rickshaw.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/carIcon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}